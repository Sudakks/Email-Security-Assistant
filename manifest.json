{
  "name": "Email Security Assistant",
  "manifest_version": 3,
  "version": "1.0.0",
  "icons": {
    "32": "icon.png"
  },

  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    //"http://*/*",
    //"https://*/*",
    "https://mail.google.com/*"
  ]
}